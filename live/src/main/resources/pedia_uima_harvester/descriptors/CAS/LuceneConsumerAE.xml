<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>hu.sztaki.pedia.uima.consumer.LuceneConsumer</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>Lucene Consumer AE</name>
    <description/>
    <version>1.0</version>
    <vendor>MTA SZTAKI</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>LuceneIndexDir</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>LangChainID</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>SofaNames</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>SkippedWordPlaceHolder</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>UnimportantPOSTags</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>LangChainID</name>
        <value>
          <string>hun_stem</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>SofaNames</name>
        <value>
          <array>
            <string>parsed</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>LuceneIndexDir</name>
        <value>
          <string>__LUCENEINDEXDIR</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>UnimportantPOSTags</name>
        <value>
          <array>
            <string>AT</string>
            <string>TO</string>
            <string>PPS</string>
            <string>PPSS</string>
            <string>CC</string>
            <string>MD</string>
            <string>UH</string>
            <string>RP</string>
            <string>WP$</string>
            <string>WPO</string>
            <string>WPS</string>
            <string>WQL</string>
            <string>WRB</string>
            <string>BE</string>
            <string>BED</string>
            <string>BEDZ</string>
            <string>BEG</string>
            <string>BEM</string>
            <string>BEN</string>
            <string>BER</string>
            <string>BEZ</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>SkippedWordPlaceHolder</name>
        <value>
          <string>XX</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="../typeSystemDescriptor.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type>org.apache.uima.TokenAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.SourceDocumentInformation</type>
          <feature>org.apache.uima.TokenAnnotation:lemma</feature>
          <feature>org.apache.uima.TokenAnnotation:posTag</feature>
        </inputs>
        <outputs/>
        <inputSofas>
          <sofaName>parsed</sofaName>
          <sofaName>rawwiki</sofaName>
        </inputSofas>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>false</modifiesCas>
      <multipleDeploymentAllowed>false</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
